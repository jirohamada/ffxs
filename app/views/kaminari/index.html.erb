<div class="header">
  <a href="/areas" class="return-top"><%="#{current_user.nickname}さんの捕獲状況"%></a>
</div>
<div class="contents">
  <div class="background-contents"></div>
  <div class="targets">

    <div class="target">
      <div class="count">サボテンダー?の捕獲数:<%= Unsabotender.includes(:user).where(user_id: current_user.id).count%></div>
      <%= form_with(url: kaminari_index_path, model: Unsabotender, method: :get, local: true) do |f| %>
        <%= f.submit "＋" , class: "btn-gradient-3d", name: "unsabotender"%> 
      <% end %>
    </div>
  </div>
</div>